stage ('Clean workspace') {
    steps {
        cleanWs()
    }
}

stage ('Git checkout') {
    steps {
        git branch: 'dev', url: 'https://github.com/GustavoVictor/simple-form.git'
    }
}

stage ('Restore packages') {
    steps {
        bat "dotnet restore ${workspace}\\simple-form\\src\\Formulario.Services.Host"
    }
}

git branch: 'dev', url: 'https://github.com/GustavoVictor/simple-form.git'